{
  "name": "top-invest-v6",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "node --max_old_space_size=8192 node_modules/@angular/cli/bin/ng serve",
    "eslint": "npx ng lint",
    "build": "ng build",
    "build-dev": "ng build --configuration=development",
    "build-recette": "ng build --configuration=recette",
    "build-preprod": "ng build --configuration=preprod",
    "build-prod": "ng build --configuration=production",
    "test-commit": "node --max_old_space_size=8192 node_modules/@angular/cli/bin/ng test --watch false",
    "watch": "ng build --watch --configuration development",
    "build:stats": "ng build --stats-json",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "test": "node --max_old_space_size=8192 node_modules/@angular/cli/bin/ng test",
    "analyze": "npm run build:stats && webpack-bundle-analyzer dist/stats.json",
    "format:fix": "pretty-quick",
    "compile:less": "webpack",
    "prepare": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-s format:fix lint test-commit",
      "pre-push": "ng build --aot true"
    }
  },
  "private": true,
